% \subsubsection*{各見出しコマンドの定義}
% この部分では，\LaTeX{} 内部コマンドの|\@startsection| と |\secdef| を
% 使用します．
% \begin{macro}{\@startsection}
% \begin{macro}{\secdef}
% |\@startsection <name><level><indent><before><after><sty>[alt]<head>|\par 
% |\secdef <cmd> <star cmd>|
% \end{macro}
% \end{macro}
