\newenvironment{titlepage}
  { \if@twocolumn\@restonecoltrue\onecolumn
    \else\@restonecolfalse\newpage
    \fi
    \thispagestyle{empty}
    \setcounter{page}\@ne }
  { \if@restonecol\twocolumn
