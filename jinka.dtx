  \let\@oddfoot\@empty
  \let\@evenfoot\@empty
  \def\@evenhead{\thepage\hfil\lrightmark}%
  \def\@oddhead{{\rightmark}\hfil\thepage}%
  \let\@mkboth\markboth
  \def\chaptermark##1{\markboth{%
      \ifnum \c@secnumdepth >\m@ne
