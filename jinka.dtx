  \let\@evenfoot\@empty
  \def\@evenhead{\hfil\rightmark}%
  \let\@oddhead\@evenhead%
  \let\@mkboth\markboth
  \def\chaptermark##1{\markboth{%
      \ifnum \c@secnumdepth >\m@ne
        \@chapapp\thechapter\@chappos\hskip 1zw
